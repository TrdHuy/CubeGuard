{
  "name": "cubeguard",
  "version": "1.0.0",
  "private": true,
  "description": "Minecraft Bedrock Add-On",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf BP/scripts",
    "rebuild": "npm run clean && npm run build",
    "dev": "tsc --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "analyze-deps": "depcruise --output-type html --output-to scripts/arch-report/dependencies.html src"
  },
  "keywords": [
    "minecraft",
    "bedrock"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@minecraft/server": "^2.3.0",
    "@minecraft/server-gametest": "^1.0.0-beta.1.21.60-preview.24",
    "@minecraft/server-ui": "^1.3.0",
    "@types/jest": "^30.0.0",
    "dependency-cruiser": "^17.3.1",
    "eslint-plugin-minecraft-linting": "^2.0.1",
    "jest": "^30.2.0",
    "madge": "^8.0.0",
    "rimraf": "^6.1.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}
