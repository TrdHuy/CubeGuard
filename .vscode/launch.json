{
  "version": "0.3.0",
  "configurations": [
    {
      "name": "Debug Jest Tests",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/jest/bin/jest.js",
        "--runInBand"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "PowerShell Launch Current File",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "cwd": "${cwd}"
    },
    {
      "type": "minecraft-js",
      "request": "attach",
      "name": "Build & Attach Debug with Minecraft (Client)",
      "mode": "listen",
      "targetModuleUuid": "5f572fd3-33b7-44fa-9d8f-001bc1997b9b",
      "localRoot": "${workspaceFolder}/BP/scripts",
      "port": 19144,
      "preLaunchTask": "Deploy All"
    },
    {
      "type": "minecraft-js",
      "request": "attach",
      "name": "Attach Debug with Minecraft (Client)",
      "mode": "listen",
      "targetModuleUuid": "5f572fd3-33b7-44fa-9d8f-001bc1997b9b",
      "sourceMapRoot": "${workspaceFolder}/src/BP/",
      "generatedSourceRoot": "${workspaceFolder}/BP/scripts/",
      "localRoot": "${workspaceFolder}/BP/scripts/",
      "port": 19144
    }
  ]
}
